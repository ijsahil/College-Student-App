<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/zenq"
        android:label="@string/app_name"
        android:roundIcon="@drawable/zenq"
        android:supportsRtl="true"
        android:theme="@style/Theme.TitClone"
        tools:targetApi="31">
        <activity
            android:name="com.example.zenQ.ChatTextActivity"
            android:theme="@style/ChatActivityTheme"
            android:exported="false" />
        <activity
            android:name="com.example.zenQ.ChatActivity"
            android:exported="false" />
        <activity
            android:name="com.example.zenQ.SignupActivity"
            android:exported="false" />
        <activity
            android:name="com.example.zenQ.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.zenQ.MainActivity"
            android:exported="false" />
    </application>

</manifest>